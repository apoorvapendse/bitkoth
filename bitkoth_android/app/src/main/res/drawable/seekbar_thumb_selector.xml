<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Default state -->
    <item android:drawable="@drawable/default_thumb" />

    <!-- Pressed state -->
    <item android:state_pressed="true" android:drawable="@drawable/pressed_thumb" />

    <!-- Focused state -->
    <item android:state_focused="true" android:drawable="@drawable/focused_thumb" />



</selector>